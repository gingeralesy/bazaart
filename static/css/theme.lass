(body
 :height "100vh"
 :width "100vw")

(.wrap
 :height "100vh"
 :width "100vw"
 :position "absolute"
 :top 0
 :bottom 0
 :left 0
 :right 0
 ((:or .page-content .navbar-content)
  :max-width "1366px"
  :width "100%"
  :margin-left "auto"
  :margin-right "auto"))

;; Box

(.bz-box
 :background-clip "padding-box"
 :border "1px solid"
 :border-radius "0.25rem"
 :padding "1rem")

(.bz-box--border
 :background-clip "padding-box"
 :border "1px solid"
 :border-radius "0.25rem"
 :padding 0)

