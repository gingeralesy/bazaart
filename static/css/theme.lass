;; Grid stuff

(.bz-flex
 :display "-ms-flexbox"
 :display "flex"
 ("> .bz-flex--top"
  :-ms-flex "0 1 auto"
  :flex "0 1 auto")
 ("> *:not(.bz-flex--top):not(.bz-flex--bottom)"
  :-ms-flex "1 1 auto"
  :flex "1 1 auto")
 ("> .bz-flex--bottom"
  :-ms-flex "0 1 40px";
  :flex "0 1 40px"))

(.bz-flex--column
 :-ms-flex-align "stretch"
 :-ms-grid-row-align "stretch"
 :align-items "stretch"
 :-ms-flex-line-pack "stretch"
 :align-content "stretch"
 :-ms-flex-direction "column"
 :flex-direction "column")
